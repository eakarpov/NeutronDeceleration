{
  "name": "NeutronDeceleration",
  "version": "0.0.1",
  "license": "MIT",
  "main": "main.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "history": "^4.7.2",
    "mini.css": "^2.3.7",
    "nedb": "^1.8.0",
    "prop-types": "^15.6.0",
    "react": "^16.1.1",
    "react-ckeditor-component": "^1.0.7",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "electron": "^1.7.9",
    "electron-devtools-installer": "^2.2.1",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "node-sass": "^4.7.2",
    "react-hot-loader": "^3.1.3",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 8
          },
          "useBuiltIns": true
        }
      ],
      "stage-0",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-class-properties",
          "transform-es2015-classes"
        ]
      }
    }
  },
  "scripts": {
    "build": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.dev.js",
    "start_electron": "electron .",
    "start": "cross-env NODE_ENV=development concurrently \"npm run build\"  \"npm run start_electron\"",
    "start:prod": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && electron ."
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.x"
  }
}
